{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "allowJs": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@/API": ["./src/API"],
      "@/API/*": ["./src/API/*"],
      "@/queries": ["./src/queries"],
      "@/queries/*": ["./src/queries/*"],
      "@/utils": ["./src/utils"],
      "@/debbuging": ["./src/debugging"],
      "@/styles": ["./src/styles"],
      "@/styles/*": ["./src/styles/*"],
      "@/navigation": ["./src/navigation"],
      "@/navigation/*": ["./src/navigation/*"],
      "@/lib/*": ["./src/lib/*"],
      "@/screens/*": ["./src/screens/*"],
      "@/screens": ["./src/screens"],
      "@/helpers": ["./src/helpers"],
      "@/helpers/*": ["./src/helpers/*"],
      "@/hooks": ["./src/hooks"]
    },
    "resolveJsonModule": true
  },
  "exclude": ["ios", "node_modules", "babel.config.js", "metro.config.js", "jest.config.js"]
}
